<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackAsTool>true</PackAsTool>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Title>PackScan</Title>
    <Authors>Jamil Geor</Authors>
    <Copyright>Jamil Geor</Copyright>
    <PackageId>PackScan</PackageId>
    <RepositoryUrl>https://github.com/jamilgeor/PackScan</RepositoryUrl>
    <PackageProjectUrl>https://github.com/jamilgeor/PackScan</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/jamilgeor/PackScan/blob/master/LICENSE</PackageLicenseUrl>
    <ToolCommandName>packscan</ToolCommandName>
    <LangVersion>latest</LangVersion>
    <Version>0.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Mono.Options" Version="5.3.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="System.Net.Http" Version="4.3.3" />
  </ItemGroup>

</Project>
